   "source": [
    "import pandas as pd \n",
    "\n",
    "data = {\n",
    "    \"반별\" : [\"1반\", \"1반\", \"2반\", \"2반\", \"3반\", \"3반\"],\n",
    "    \"성적\" : [80, 90, 70, 60, 100, 70]\n",
    "}\n",
    "\n",
    "# 데이터 프레임 생성\n",
    "df = pd.DataFrame(data)\n",
    "df\n",
    "\n",
    "# 그룹화 및 통계\n",
    "describe = df.groupby(\"반별\").agg(['count', 'mean', 'min', 'max'])\n",
    "print(describe)"
   ]
  }
